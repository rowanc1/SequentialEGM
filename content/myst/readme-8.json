  "sha256": "4227c8bcbb23585ae2aa200a549f2b3ee5b474f7b39afd3f1ad7a8cc349a89cf",
  "slug": "readme-8",
  "mdast": {
    "type": "root",
    "children": [
      {
        "type": "block",
        "children": [
          {
            "type": "paragraph",
            "position": {
              "start": { "line": 2, "column": 0 },
              "end": { "line": 5, "column": 0 }
            },
            "children": [
              {
                "type": "text",
                "value": "If you use TeXLive on more than one system, or under more than one username,\nit is useful to have a set of customizations that are shared across all your\nidentities and machines.",
                "key": "BzbK72o4XJ"
              }
            ],
            "key": "chYl6MXxA5"
          },
          {
            "type": "paragraph",
            "position": {
              "start": { "line": 6, "column": 0 },
              "end": { "line": 7, "column": 0 }
            },
            "children": [
              {
                "type": "text",
                "value": "This can be accomplished in a number of ways.",
                "key": "cRmGnLqI8W"
              }
            ],
            "key": "r3cvG7BTNC"
          },
          {
            "type": "paragraph",
            "position": {
              "start": { "line": 8, "column": 0 },
              "end": { "line": 11, "column": 0 }
            },
            "children": [
              {
                "type": "text",
                "value": "The easiest is probably to install a file sync service like Dropbox that runs\nautomatically on all of the machines/users in question, and to have a master\nversion of texmf-local that lives in Dropbox.",
                "key": "E92v2z6nnK"
              }
            ],
            "key": "SGlrjWgd6U"
          },
          {
            "type": "paragraph",
            "position": {
              "start": { "line": 12, "column": 0 },
              "end": { "line": 13, "column": 0 }
            },
            "children": [
              {
                "type": "text",
                "value": "Then you have two options:",
                "key": "HkLUKUsG2Q"
              }
            ],
            "key": "NYzkh3Qa8s"
          },
          {
            "type": "list",
            "ordered": true,
            "start": 1,
            "spread": false,
            "position": {
              "start": { "line": 14, "column": 0 },
              "end": { "line": 19, "column": 0 }
            },
            "children": [
              {
                "type": "listItem",
                "spread": true,
                "position": {
                  "start": { "line": 14, "column": 0 },
                  "end": { "line": 16, "column": 0 }
                },
                "children": [
                  {
                    "type": "text",
                    "value": "On unix-based machines (Mac or Linux, say), you can replace the texmf-local\nfile on your machine with a symbolic link to the filepath in Dropbox",
                    "key": "AatRieMTG6"
                  }
                ],
                "key": "UKHHKIvBSC"
              },
              {
                "type": "listItem",
                "spread": true,
                "position": {
                  "start": { "line": 16, "column": 0 },
                  "end": { "line": 19, "column": 0 }
                },
                "children": [
                  {
                    "type": "text",
                    "value": "Windows does not allow symbolic links. So on Windows machines you will need\nto modify the texmf.cnf configuration file so that it knows to look in the\nright place (your Dropbox path) for the texmf-local config files",
                    "key": "c9K5OpGJBz"
                  }
                ],
                "key": "ey56ThKWGW"
              }
            ],
            "key": "HLsqmx75ok"
          }
        ],
        "key": "RrNvl8aEr7"
      }
    ],
    "key": "xLR6Tc3Ztm"
  },
  "references": { "cite": { "order": [], "data": {} } }
}
